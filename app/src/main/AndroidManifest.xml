<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wjn.viewlistdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.attribute.AnimationDrawableActivity"/>
        <activity android:name=".activity.attribute.AnimationDrawableActivitys"/>
        <activity android:name=".activity.viewpagerfragment.MainVFActivity" />
        <activity android:name=".activity.recyclerview.RecyclerViewActivity" />
        <activity android:name=".activity.textview.TextViewRollingActivity" />
        <activity android:name=".activity.textview.TextViewMaxActivity" />
        <activity android:name=".activity.textview.TextViewActivity" />
        <activity android:name=".activity.tablayout.TabLayoutActivity" />
        <activity android:name=".activity.viewpager2tablayout.ViewPager2Activity" />
        <activity android:name=".activity.viewpagertablayout.ViewPagerTabLayoutActivity" />
        <activity android:name=".activity.layout.LayoutShowHiddenActivity" />
        <activity android:name=".activity.layout.LayoutShowHiddenActivitys" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>